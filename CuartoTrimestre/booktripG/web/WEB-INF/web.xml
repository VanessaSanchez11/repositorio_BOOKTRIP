<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
   
   <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>servlet.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>registerServices</servlet-name>
        <servlet-class>servlet.registerServices</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServices</servlet-name>
        <url-pattern>/registerServices</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>registroPagos</servlet-name>
        <servlet-class>servlet.registroPagos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registroPagos</servlet-name>
        <url-pattern>/registroPagos</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>consultarPagos</servlet-name>
        <servlet-class>servlet.consultarPagos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>consultarPagos</servlet-name>
        <url-pattern>/consultarPagos</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>actualizarPagos</servlet-name>
        <servlet-class>servlet.actualizarPagos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>actualizarPagos</servlet-name>
        <url-pattern>/actualizarPagos</url-pattern>
    </servlet-mapping>    
    
    <servlet>
        <servlet-name>registrarPuntuaciones</servlet-name>
        <servlet-class>servlet.registrarPuntuaciones</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registrarPuntuaciones</servlet-name>
        <url-pattern>/registrarPuntuaciones</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>registroReservas</servlet-name>
        <servlet-class>servlet.registroReservas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registroReservas</servlet-name>
        <url-pattern>/registroReservas</url-pattern>
    </servlet-mapping>
    
    
 
    
    
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>servlet.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>   
    
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
